@{
    ViewData["Title"] = "Agregar Servicio";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - RapiSolverWeb</title>
<title>Bootstrap Example</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
</head>
<body>
 <header>
        <nav class="navbar navbar-inverse">
  <div class="container-fluid">
    <div class="navbar-header">
      <a class="navbar-brand"asp-area="" asp-controller="Home" asp-action="Index">RapiSolver</a>
    </div>
    <ul class="nav navbar-nav">
      
      <li><a href="#">Iniciar Sesion</a></li>
      <li><a class="nav-link text-dark" asp-area="" asp-controller="Service" asp-action="list" >Buscar Servicio</a></li>
      <li><a class="nav-link text-dark" asp-area="" asp-controller="Service" asp-action="list">Crear Servicio</a></li>
      <li><a href="#">Buscar Persona</a></li>
      
    </ul>
  </div>
</nav>
    </header>

    @Model Servicio
    <form asp-action="agregar" asp-controller="Service" method="post">
     <div class="row">
         <div class="form-group col">
            <label>Nombre:</label>
            <input type="text"  class="form-control">
           
        </div>
        <div class="form-group col">
            <label>Nombre:</label>
            <input type="text"  class="form-control">
           
        </div>
        <div class="form-group col">
            <label>Descripcion:</label>
            <input type="text"  class="form-control">
        </div>
         <div class="form-group col">
            <label>Costo:</label>
            <input type="text"  class="form-control">
        </div>
        <div class="form-group col">
            <label>Categoria:</label>
            <input type="text"  class="form-control">
        </div>
        <div class="row">
        <button type="submit" class="btn btn-primary">Guardar</button>
    </div>
    </div>
</form>

</body>

</html>